
fn foobar<T, U>(x: T, y: U) -> void {
        print x;
        print y;
}

foobar<int, int>(3, 4);
foobar<str, str>("hello", "there");
foobar<bool, bool>(true, false);

fn add<T>(first: T, second: T) -> T {
        return first + second;
}

print add<int>(69, 420);
print add<float>(1.2, 3.4);
print add<str>("foo", "bar");