fn add(x: int, y: int) -> int {
    var a = x + 3;
    var b = y;
    return true ? a + b : -1;
}

fn double(x: int) -> int {
    return x * 2;
}

fn quadruple(x: int) -> int {
    return double(double(x));
}

fn print_if(input: bool) -> void {
    if (input) {
        print 1;
    } else {
        print 0;
    }
}

